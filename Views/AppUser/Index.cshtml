@model IEnumerable<smart_city_dotnetcore.Models.AppUser>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Date Of Birth</th>
            <th>Gender</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var appUser in Model){
           <tr>
                <td>@appUser.FirstName</td>
                <td>@appUser.MiddleName</td>
                <td>@appUser.LastName</td>
                <td>@appUser.DateOfBirth</td>
                <td>@appUser.Gender</td>
                <td>@appUser.Image</td>
           </tr> 
        }
    </tbody>
</table>
